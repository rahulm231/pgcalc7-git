<script>
    $(function () {
        $(".left-nav-wrap .panel-default").each(function () {
            if ($(this).hasClass("active")) {
                $(this).addClass("active-parent");
                $(this).find("span").addClass("close");
            }
            else {
                $(this).find("span").addClass("open");
            }

            $(this).find("span").click(function () {
                $(this).parents(".left-nav-wrap").children(".panel-default").each(function () {
                    if (!$(this).hasClass("active-parent")) {
                        $(this).removeClass("active");
                    }
                    $(this).find("span").removeClass("close").addClass("open");
                });

                if ($(this).hasClass("open")) {
                    $(this).removeClass("open").addClass("close");
                }
                else {
                    $(this).removeClass("close").addClass("open");
                }
            });
        });

		$("[data-nodeid='8']").find(".list-unstyled:first > li").each(function (i) {
			$(this).find("a").first().text("- " + $(this).find("a").first().text());
		});

        $(".panel-default:first").addClass("first");
        $(".panel-default.active-parent").prev().addClass("bottom-red");
    });
</script>





      
