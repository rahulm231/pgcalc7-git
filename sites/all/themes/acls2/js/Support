        var key = e.which;
        if(key == 13)  // the enter key code
        {
          $('#btnSearch').click();
          return false;  
        }
      });
    
      if($('a').length > 0) {
        $('a').filter(function() {
          var attr = $(this).attr('href');
          if (typeof attr !== typeof undefined && attr !== false) {
              return $(this).attr('href').match(/\.(pdf|doc|docx|xls|xlsx|rtf|txt)/i);
            }
        }).attr('target','_blank');
      }
    
