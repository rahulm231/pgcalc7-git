      if(href.indexOf("/ACLS/media/GlobalMediaLibrary/Publications/") >= 0){
        href = href.replace("/ACLS/media/GlobalMediaLibrary/Publications/", "http://publications.acls.org/");
      }
      else if(href.indexOf("/ACLS/media/GlobalMediaLibrary/") >= 0){
        href = href.replace("/ACLS/media/GlobalMediaLibrary/", "http://documents.acls.org/");
      }
      
      $(this).attr("href", href);
    });
    
    $(".redirect-me").each(function(){
      $(this).click(function(){
