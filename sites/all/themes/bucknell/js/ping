"use strict";new function(){var s,s2;if(window["ping_admissions.bucknell.edu"]==null){window["ping_admissions.bucknell.edu"]=!0;var ping_service="https://admissions.bucknell.edu/ping?cmd=register",ping_start=(new Date).getTime();window.addEventListener("unload",function(){var ping_duration=(new Date).getTime()-ping_start,url=ping_service+"&d="+encodeURIComponent(ping_duration.toString())+"&t="+(new Date).getTime().toString(),client,s,s2;document.referrer!=null&&(url+="&r="+encodeURIComponent(document.referrer));try{navigator.sendBeacon?navigator.sendBeacon(url,""):(client=new XMLHttpRequest,client.open("GET",url,!1),client.send())}catch(ex){s=document.getElementsByTagName("SCRIPT")[0];s2=document.createElement("SCRIPT");s2.async=!0;s2.src=url;s.parentNode.insertBefore(s2,s)}});window.ping_whoami!=null&&(s=document.getElementsByTagName("SCRIPT")[0],s2=document.createElement("SCRIPT"),s2.async=!0,s2.src="https://admissions.bucknell.edu/ping?cmd=whoami",s.parentNode.insertBefore(s2,s))}};